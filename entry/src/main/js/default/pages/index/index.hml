<div class="container">
    <swiper class="container" loop="false">
        <div class="swiper" for="{{ qrCodeList }}">
            <div class="swiper" if="{{ qrCodeList.length > 0 }}">
                <stack class="swiper">
                    <qrcode value="{{ $item }}" class="qrcode" on:click="showValue({{$idx}})">
                    </qrcode>
                </stack>
            </div>
        </div>
    </swiper>
</div>